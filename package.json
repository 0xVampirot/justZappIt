{
  "name": "justzappit",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "description": "A community-driven, open-source directory of physical crypto exchange shops worldwide.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xVampirot/justZappIt.git"
  },
  "bugs": {
    "url": "https://github.com/0xVampirot/justZappIt/issues"
  },
  "homepage": "https://justzappit.xyz",
  "author": "justZappIt Contributors",
  "keywords": [
    "crypto",
    "exchange",
    "map",
    "directory",
    "web3"
  ],
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/web",
    "build": "npm run build --workspace=apps/web",
    "lint": "npm run lint --workspace=apps/web",
    "test": "npm run test --workspace=apps/web",
    "type-check": "npx tsc --noEmit --project apps/web/tsconfig.json",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "seed": "npm run seed --workspace=packages/db"
  },
  "devDependencies": {
    "dotenv": "^17.3.1"
  }
}
